{
   "name":"dennisdigital/drupal-example-site",
   "description": "Example of building a D7 site with Drupal Console Commands.",
   "type":"project",
   "minimum-stability":"dev",
   "prefer-stable":true,
   "repositories":{
      "pdo":{
         "type":"composer",
         "url":"https://packages.drupal.org/7"
      },
      "drupal":{
         "type":"package",
         "package":{
            "name":"drupal/core",
            "version":"7.56",
            "type":"drupal-core",
            "source":{
               "url":"https://git.drupal.org/project/drupal.git",
               "type":"git",
               "reference":"7.56"
            },
            "dist":{
               "url":"https://ftp.drupal.org/files/projects/drupal-7.56.zip",
               "type":"zip",
               "skip-dev":true
            }
         }
      },
      "ckeditor":{
         "type":"package",
         "package":{
            "name":"lib/ckeditor",
            "version":"4.6.1",
            "type":"drupal-library",
            "dist":{
               "url":"https://download.cksource.com/CKEditor/CKEditor/CKEditor%204.6.1/ckeditor_4.6.1_full.zip",
               "type":"zip",
               "reference":"master"
            }
         }
      },
      "jquery.cycle":{
         "type":"package",
         "package":{
            "name":"lib/jquery.cycle",
            "version":"3.0.3",
            "type":"drupal-library",
            "source":{
               "url":"https://github.com/malsup/cycle.git",
               "type":"git",
               "reference":"3.0.3"
            }
         }
      },
      "rrssb":{
         "type":"package",
         "package":{
            "name":"lib/rrssb",
            "version":"1.6.0",
            "type":"drupal-library",
            "source":{
               "url":"https://github.com/kni-labs/rrssb",
               "type":"git",
               "reference":"v1.6.0"
            }
         }
      },
      "flexslider":{
         "type":"package",
         "package":{
            "name":"lib/flexslider",
            "version":"2.6.3",
            "type":"drupal-library",
            "source":{
               "url":"https://github.com/woocommerce/FlexSlider",
               "type":"git",
               "reference":"master"
            }
         }
      }
   },
   "require":{
      "drupal/core":"~7",
      "composer/installers":"^1.2",
      "drupal/rubik":"^4.0",
      "lib/ckeditor":"^4.6",
      "lib/jquery.cycle":"^3.0",
      "lib/rrssb":"^1.6",
      "lib/flexslider":"^2.6",
      "drupal/sharerich":"^2.5",
      "drupal/colorbox":"^2.12",
      "drupal/admin_menu":"^3.0@RC",
      "drupal/advagg":"^2.2",
      "drupal/captcha_keypad":"1.x-dev",
      "drupal/devel":"^1.5",
      "drupal/entity":"^1.8",
      "drupal/entitycache":"^1.5",
      "drupal/entityreference":"^1.2",
      "drupal/features":"^2.10",
      "drupal/views":"^3.18",
      "drupal/views_bulk_operations":"^3.4",
      "drupal/memcache":"^1.6",
      "drupal/pathauto":"^1.3",
      "drupal/robotstxt":"^1.4",
      "drupal/token":"^1.7",
      "drupal/token_custom":"^1.1",
      "drupal/token_filter":"^1.1",
      "drupal/variable":"^2.5",
      "drupal/views_slideshow":"^3.8",
      "drupal/xmlsitemap":"^2.3",
      "drupal/nodequeue":"^2.1",
      "drupal/jquery_update":"^2.7",
      "phpunit/phpunit":"> 4.8.28 <5",
      "drupal/stage_file_proxy": "^1.7",
      "drupal/queue_ui": "^2.0@RC"
   },
   "require-dev":{
      "drupal/drupal-extension":"~3.2.0",
      "behat/mink":"^1.7",
      "behat/mink-goutte-driver":"~1.2.0",
      "behat/mink-selenium2-driver":"dev-master#721cbbaf37f71434f6acb604dc3a17a6a9be0c4c"
   },
   "scripts":{
      "post-install-cmd":[
         "@composer drupal-scaffold",
         "rm -rf ./core; cd ./web/core; rm -rf sites; ln -s ../sites; git checkout .htaccess"
      ],
      "post-update-cmd":[
         "@composer drupal-scaffold",
         "rm -rf ./core; cd ./web/core; rm -rf sites; ln -s ../sites; git checkout .htaccess"
      ],
      "drupal-scaffold":"DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
   },
   "extra":{
      "installer-paths":{
         "web/core":[
            "drupal/core"
         ],
         "web/profiles/contrib/{$name}":[
            "type:drupal-profile"
         ],
         "web/sites/default/libraries/{$name}":[
            "type:drupal-library"
         ],
         "web/sites/default/modules/contrib/{$name}":[
            "type:drupal-module"
         ],
         "web/sites/default/themes/contrib/{$name}":[
            "type:drupal-theme"
         ]
      },
      "drupal-scaffold":{
         "excludes":[
            "web.config"
         ]
      }
   }
}
